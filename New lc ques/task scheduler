#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int c = 0;

        while (n > 0) {
            if (n >= 15) {
                n -= 15;
                c++;
            } else if (n >= 10) {
                n -= 10;
                c++;
            } else if (n >= 6) {
                n -= 6;
                c++;
            } else if (n >= 3) {
                n -= 3;
                c++;
            } else if (n >= 1) {
                n -= 1;
                c++;
            }
        }
        cout << c << endl;
    }
    return 0;
}