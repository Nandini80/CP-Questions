
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
 
int main(){
	int t;
	cin>>t;
	
	while(t--){
		int n,m ;
		cin>>n>>m;
		
		
		string s="",t="";
		cin>>s;
		
		for(int i=1;i<2*n-1;i++){
			cin>>t;
			
			for(int j=0;j<m;j++){
				s[j] ^= t[j];
			}
		}
		cout<<s;
		cout<<"\n";
		
	} 
	return 0;
}