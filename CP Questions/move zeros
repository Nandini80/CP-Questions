class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        vector<int> ps;

        for ( int i = 0; i < nums.size();i++){
            if ( nums[i] != 0){
                ps.push_back(nums[i]);
            }
        }
        while ( ps.size() != nums.size()){
            ps.push_back(0);
        }
        nums = ps;
    }
};