#include <bits/stdc++.h>
using namespace std;
#define ll long long
 
void solve() {
	int n, q;
	cin >> n >> q;
	vector<ll> a(n+1), b(n+1);
	ll cur = 0;
	for (int i = 1; i <= n; ++i) {
		ll k; cin >> k;
		a[i] = max(a[i - 1], k);
 
		cur += k;
		b[i] = cur;
	}
 
	while (q--) {
		int k; cin >> k;
		
		int idx = upper_bound(begin(a), end(a), k) - begin(a) - 1;
		cout << b[idx] << ' ';
	}
	cout << endl;
}
 
int main() {
	int t; cin >> t;
	while (t--)
		solve();
 
	return 0;
}