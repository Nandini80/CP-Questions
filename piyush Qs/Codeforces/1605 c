#include<bits/stdc++.h>
using namespace std;
void solve(){
	int n,i;
    cin>>n;
	string s,ans[7]={"aa","aba","aca","abca","acba","abbacca","accabba"};
	cin>>s;
	int res=-1;
	for(int i=0;i<7;i++){
		if(s.find(ans[i])!=-1){
			res=ans[i].length();
			break;
		}
	}
	cout<<res<<endl;
}
int main(){
	int t;
	cin>>t;
	while(t--){
		solve();
	}
	return 0;
}