#include<bits/stdc++.h>
using namespace std;
 
int main(){
	ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
	int t,n;
	cin>>t;
	string s;
	while(t--){
		cin>>n>>s;
		sort(s.begin(),s.end());
		int b=0,e=n-1;
		while(b<=e){
			if(b==e){
					cout<<s[b];
			}
			else cout<<s[b]<<s[e];
			++b;
			--e;
		}
		cout<<'\n';
	}
}