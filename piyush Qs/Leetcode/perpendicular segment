#include <iostream>
using namespace std;
 
signed main() {
    int t; 
    cin >> t;
    while (t--) {
        int a, b, k; 
        cin >> a >> b >> k;
 
        int val;
        if (a < b) {
            val = a;
        } else {
            val = b;
        }
 
        cout << "0 " << val << " " << val << " 0" << endl;
        cout << "0 0 " << val << " " << val << endl;
    }
    return 0;
}