class Solution {
public:
    string defangIPaddr(string add) {
        string newadd;
        int len = add.length();
        for ( int i = 0; i < len; i++){
            if ( add[i] == '.'){
                newadd +="[.]";
            }
            else {
                newadd += add[i];
            }
            cout<<newadd<< endl;
        }
        return newadd;
    }
};